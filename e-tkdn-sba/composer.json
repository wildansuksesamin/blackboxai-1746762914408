{
    "name": "codeigniter3/appstarter",
    "description": "CodeIgniter3 starter app",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.3",
        "codeigniter/framework": "3.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "application/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit"
    }
}
